    "@nestjs/cli": "11.0.10",
    "@nestjs/schematics": "11.0.9",
    "@nestjs/testing": "11.1.6",
    "@swc-node/register": "1.11.1",
    "@swc/cli": "0.7.8",
    "@swc/core": "1.13.20",
