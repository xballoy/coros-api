  },
  "homepage": "https://github.com/xballoy/coros-api#readme",
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@nestjs/cli": "11.0.4",
    "@nestjs/schematics": "11.0.1",
    "@nestjs/testing": "11.0.10",
    "@swc-node/register": "1.10.9",
    "@swc/cli": "0.6.0",
    "@swc/core": "1.11.1",
    "@tsconfig/node22": "22.0.0",
    "@types/node": "22.13.5",
    "@vitest/coverage-v8": "3.0.7",
    "fast-check": "3.23.2",
    "lefthook": "1.11.0",
