  "devDependencies": {
    "@biomejs/biome": "2.1.3",
    "@nestjs/cli": "11.0.10",
    "@nestjs/schematics": "11.0.7",
    "@nestjs/testing": "11.1.5",
    "@swc-node/register": "1.10.10",
    "@swc/cli": "0.7.8",
    "@swc/core": "1.13.3",
    "@tsconfig/node22": "22.0.2",
    "@types/node": "22.16.5",
